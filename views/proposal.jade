extends layout

include mixins/assets

block content
  h1 Proposal #{asset.data.propid}

  mixin versionInfo(snapshot, metadata.navigation)

  p

  span(style="float:right")
    a(href="/edit/#{metadata.base.xlink}") edit

  h2 title
  !=asset.getTitle()

  h2 type
  !=asset.getProposalType()

  h2 sponsor(s)
  !=asset.getSponsors()

  h2 rationale
  !=asset.getRationale()
